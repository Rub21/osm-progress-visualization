#!/bin/bash
for i in $(seq $1 $2)
do	
	#curl -O http://planet.openstreetmap.org/replication/day/000/000/$i.osc.gz
	path_files=""
 	gunzip -d $path_files$i.osc.gz
	node index.js --osmfile=$i.osc --boundfile=$3 --path=$path_files
	gzip -c $path_files$i.osc > $path_files$i.osc.gz
	rm $path_files$i.osc
done

